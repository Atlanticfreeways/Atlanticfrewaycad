{
  "name": "atlanticfrewaycard-platform",
  "version": "1.0.0",
  "description": "Unified card platform combining business expense management and personal virtual cards",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:property": "jest --testPathPattern=property",
    "test:jit": "jest tests/unit/services/marqeta/JITFundingService.test.js tests/integration/services/JITFundingService.test.js tests/unit/services/marqeta/JITFundingService.property.test.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "migrate": "psql $DATABASE_URL -f database/migrations/001_enhanced_schema.sql",
    "migrate:all": "for f in database/migrations/*.sql; do psql $DATABASE_URL -f $f; done",
    "docker:build": "docker build -t atlanticfrewaycard .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f app"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.984.0",
    "amqplib": "^0.10.3",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parse": "^6.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-http-context": "^2.0.1",
    "express-rate-limit": "^7.1.5",
    "finalhandler": "^2.1.1",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
    "jsdom": "^27.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "pg-query-stream": "^4.12.0",
    "prom-client": "^15.0.0",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.3",
    "redis": "^4.7.1",
    "socket.io": "^4.8.3",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.18.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fast-check": "^3.14.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "socket.io-client": "^4.8.3",
    "supertest": "^6.3.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
